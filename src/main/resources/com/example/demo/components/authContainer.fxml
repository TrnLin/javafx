<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.ComponentsController.HeaderController">
   <children>
      <HBox alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" spacing="8.0">
         <children>
            <Button fx:id="notiBtn" mnemonicParsing="false" styleClass="button-second" text="Notification" />
            <Button fx:id="authBtn" mnemonicParsing="false" onAction="#checkAuth" styleClass="button-second" text="Button" />
         </children>
      </HBox>
   </children>
</VBox>
